local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local Lighting = game:GetService("Lighting")
local player = Players.LocalPlayer

if player.PlayerGui:FindFirstChild("VTD_Intro") then
    player.PlayerGui.VTD_Intro:Destroy()
end

local gui = Instance.new("ScreenGui")
gui.Name = "VTD_Intro"
gui.IgnoreGuiInset = true
gui.ResetOnSpawn = false
gui.Parent = player:WaitForChild("PlayerGui")

local blur = Instance.new("BlurEffect")
blur.Size = 0
blur.Parent = Lighting

local bg = Instance.new("Frame")
bg.Parent = gui
bg.Size = UDim2.fromScale(1,1)
bg.BackgroundColor3 = Color3.fromRGB(10,10,15)
bg.BackgroundTransparency = 1

local title = Instance.new("TextLabel")
title.Parent = gui
title.AnchorPoint = Vector2.new(0.5,0.5)
title.Position = UDim2.fromScale(0.5,0.5)
title.Size = UDim2.fromOffset(700,140)
title.BackgroundTransparency = 1
title.Text = "VTD ROBLOX"
title.Font = Enum.Font.GothamBlack
title.TextScaled = true
title.TextColor3 = Color3.fromRGB(0,255,255)
title.TextTransparency = 1
title.TextStrokeTransparency = 1

local sub = Instance.new("TextLabel")
sub.Parent = gui
sub.AnchorPoint = Vector2.new(0.5,0.5)
sub.Position = UDim2.fromScale(0.5,0.62)
sub.Size = UDim2.fromOffset(500,60)
sub.BackgroundTransparency = 1
sub.Text = "Loading Script..."
sub.Font = Enum.Font.GothamMedium
sub.TextScaled = true
sub.TextColor3 = Color3.fromRGB(200,200,200)
sub.TextTransparency = 1

local ring = Instance.new("Frame")
ring.Parent = gui
ring.AnchorPoint = Vector2.new(0.5,0.5)
ring.Position = UDim2.fromScale(0.5,0.5)
ring.Size = UDim2.fromOffset(220,220)
ring.BackgroundTransparency = 1

local uiStroke = Instance.new("UIStroke")
uiStroke.Parent = ring
uiStroke.Thickness = 3
uiStroke.Color = Color3.fromRGB(0,255,255)
uiStroke.Transparency = 1

Instance.new("UICorner", ring).CornerRadius = UDim.new(1,0)

TweenService:Create(bg, TweenInfo.new(1), {BackgroundTransparency = 0}):Play()
TweenService:Create(blur, TweenInfo.new(1), {Size = 18}):Play()
TweenService:Create(title, TweenInfo.new(1.2, Enum.EasingStyle.Quint), {
    TextTransparency = 0,
    TextStrokeTransparency = 0.3
}):Play()
TweenService:Create(sub, TweenInfo.new(1.2), {TextTransparency = 0}):Play()
TweenService:Create(uiStroke, TweenInfo.new(1), {Transparency = 0}):Play()

local rot = 0
local conn = RunService.RenderStepped:Connect(function(dt)
    rot += dt * 90
    ring.Rotation = rot
end)

task.wait(4)

TweenService:Create(title, TweenInfo.new(0.8), {TextTransparency = 1, TextStrokeTransparency = 1}):Play()
TweenService:Create(sub, TweenInfo.new(0.8), {TextTransparency = 1}):Play()
TweenService:Create(uiStroke, TweenInfo.new(0.8), {Transparency = 1}):Play()
TweenService:Create(bg, TweenInfo.new(0.8), {BackgroundTransparency = 1}):Play()
TweenService:Create(blur, TweenInfo.new(0.8), {Size = 0}):Play()

task.wait(1)

conn:Disconnect()
blur:Destroy()
gui:Destroy()

repeat task.wait() until game:IsLoaded()

local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local Window = Fluent:CreateWindow({
    Title = "vietdepzaicuto",
    SubTitle = "Blox Fruit",
    TabWidth = 157,
    Size = UDim2.fromOffset(450,300),
    Acrylic = true,
    Theme = "Light"
